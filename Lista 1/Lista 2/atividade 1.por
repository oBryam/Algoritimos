programa {
  funcao inicio() {
            // Declarando as variáveis
        inteiro a, b

        // Solicitando os valores de a e b
        escreva("Digite o valor de a: ")
        leia(a)
        
        escreva("Digite o valor de b: ")
        leia(b)
        
        // Comparando as variáveis
        se (a == b) {
            escreva("a e b são iguais.")
        } senao {
            escreva("a e b são diferentes.")
        }
  }
}
